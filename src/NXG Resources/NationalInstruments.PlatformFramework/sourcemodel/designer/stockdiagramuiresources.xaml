				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<DataTemplate
		x:Key="BaseCommandParameterTemplate">
		<StackPanel
			Name="BaseCommandParameterTemplate_StackPanel"
			Orientation="Horizontal">
			<coreConverters:ImageMask
				Name="PART_Image"
				Margin="0,0,5,0"
				Stretch="None"
				Source="{Binding SmallImageSource}" />
			<TextBlock
				Name="PART_Text"
				VerticalAlignment="Center"
				Text="{Binding LabelTitle}" />
		</StackPanel>
		<DataTemplate.Triggers>
			<Trigger
				SourceName="PART_Image"
				Property="Source"
				Value="{x:Null}">
				<Setter
					TargetName="PART_Image"
					Property="Visibility"
					Value="Collapsed" />
				<Setter
					TargetName="PART_Text"
					Property="Margin"
					Value="8,0,0,0" />
			</Trigger>
			<DataTrigger
				Value="True"
				Binding="{Binding IsSelected, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ComboBoxItem}}}">
				<Setter
					TargetName="PART_Image"
					Property="coreControls:ImageMask.Brush"
					Value="{Binding (coreControls:CoreControlStyling.IconFocusedBrush), RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type ComboBox}}}" />
			</DataTrigger>
		</DataTemplate.Triggers>
	</DataTemplate>
</ResourceDictionary>

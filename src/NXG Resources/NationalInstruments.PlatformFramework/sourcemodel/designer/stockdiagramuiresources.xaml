	<Style
		TargetType="{x:Type Control}"
		x:Key="Stock_Structure">
		<Setter
			Property="Panel.ZIndex"
			Value="{Binding Source={x:Static smDesigner:DiagramZIndex.Structure}, Converter={StaticResource DiagramZIndexToIntConverter}}" />
		<Setter
			Property="Background"
			Value="#00FFFFFF" />
		<Setter
			Property="FrameworkElement.FocusVisualStyle"
			Value="{x:Null}" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Control}">
					<Grid
						Name="LayoutRoot"
						Background="#00FFFFFF">
						<smDesigner:DiagramCanvas
							Name="NestedDiagram"
							Background="#10FFFFFF"
							DataContext="{Binding Path=NestedDiagram}"
							Margin="{Binding DataContext.DiagramMargin, ElementName=LayoutRoot, Mode=OneTime}" />
						<core:NineGrid
							IsHitTestVisible="False"
							Data="{Binding ForegroundImageData}"
							Opacity="{Binding BorderOpacity}" />
						<Border
							BorderBrush="#00000000"
							CornerRadius="{Binding OverlayCornerRadius, Mode=OneTime}"
							BorderThickness="{Binding OverlayThickness, Mode=OneTime}"
							Background="{x:Null}" />
						<smDesigner:DiagramCanvas
							IsHitTestVisible="True"
							DataContext="{Binding Path=BorderNodeDiagram}"
							Background="{x:Null}"
							Opacity="{Binding DataContext.Opacity, ElementName=LayoutRoot}" />
						<ContentPresenter
							Content="{Binding Content}" />
						<smDesigner:WatermarkTextControl
							TextOptions.TextFormattingMode="Ideal"
							FontSize="14"
							Margin="0,1,22,0"
							Text="{Binding ElementName=NestedDiagram, Path=DocumentLanguageName}" />
						<core:NineGrid
							DataSource="{Binding GlyphData}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style
		TargetType="{x:Type Control}"
		x:Key="Stock_MultipleDiagramStructure">
		<Setter
			Property="Panel.ZIndex"
			Value="{Binding Source={x:Static smDesigner:DiagramZIndex.Structure}, Converter={StaticResource DiagramZIndexToIntConverter}}" />
		<Setter
			Property="Background"
			Value="#00FFFFFF" />
		<Setter
			Property="FrameworkElement.FocusVisualStyle"
			Value="{x:Null}" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type Control}">
					<Grid>
						<Rectangle
							Name="MDSRectTop"
							Height="5"
							VerticalAlignment="Top"
							StrokeThickness="1.5"
							Stroke="{Binding ViewColor}">
							<Shape.Fill>
								<LinearGradientBrush
									EndPoint="16.288,3.495"
									StartPoint="7.272,3.43"
									SpreadMethod="Repeat"
									MappingMode="Absolute">
									<GradientStop
										Offset="0"
										Color="{Binding ViewColor}" />
									<GradientStop
										Color="White"
										Offset="0.501" />
									<GradientStop
										Offset="0.5"
										Color="{Binding ViewColor}" />
								</LinearGradientBrush>
							</Shape.Fill>
						</Rectangle>
						<Rectangle
							Height="5"
							VerticalAlignment="Bottom"
							StrokeThickness="1.5"
							Stroke="{Binding ViewColor}">
							<Shape.Fill>
								<LinearGradientBrush
									EndPoint="16.288,3.495"
									StartPoint="7.272,3.43"
									SpreadMethod="Repeat"
									MappingMode="Absolute">
									<GradientStop
										Offset="0"
										Color="{Binding ViewColor}" />
									<GradientStop
										Color="White"
										Offset="0.501" />
									<GradientStop
										Offset="0.5"
										Color="{Binding ViewColor}" />
								</LinearGradientBrush>
							</Shape.Fill>
						</Rectangle>
						<ItemsControl
							Name="_Frames"
							HorizontalAlignment="Left"
							Margin="5,0,0,0"
							ItemsSource="{Binding Path=NestedDiagrams}">
							<ItemsControl.ItemsPanel>
								<ItemsPanelTemplate>
									<StackPanel
										Orientation="Horizontal" />
								</ItemsPanelTemplate>
							</ItemsControl.ItemsPanel>
							<ItemsControl.ItemTemplate>
								<DataTemplate>
									<Grid
										Background="#00FFFFFF"
										Margin="-5,0,0,0">
										<Rectangle
											Name="_frame"
											Tag="frame"
											Margin="5,0,5,0"
											Height="5"
											VerticalAlignment="Top"
											IsHitTestVisible="True" />
										<Rectangle
											Name="_frame2"
											Tag="frame"
											Margin="5,0,5,0"
											Height="5"
											VerticalAlignment="Bottom"
											IsHitTestVisible="True" />
										<Rectangle
											Name="_between"
											Tag="between"
											Margin="0,5,0,5"
											Width="5"
											HorizontalAlignment="Left"
											IsHitTestVisible="True"
											Fill="{Binding Path=Stroke, ElementName=MDSRectTop}" />
										<Rectangle
											Name="_after"
											Tag="after"
											Margin="0,5,0,5"
											Width="5"
											HorizontalAlignment="Right"
											IsHitTestVisible="True"
											Fill="{Binding Path=Stroke, ElementName=MDSRectTop}" />
										<smDesigner:DiagramCanvas
											Name="_Diagram"
											Background="#A0FFFFFF"
											Margin="5,5,5,5" />
									</Grid>
								</DataTemplate>
							</ItemsControl.ItemTemplate>
						</ItemsControl>
						<smDesigner:DiagramCanvas
							IsHitTestVisible="True"
							DataContext="{Binding Path=BorderNodeDiagram}"
							Background="{x:Null}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style
		TargetType="{x:Type smDesigner:TextContentElementView}"
		x:Key="Stock_NineGridBackgroundTextContent">
		<Setter
			Property="FrameworkElement.FocusVisualStyle"
			Value="{x:Null}" />
		<Setter
			Property="Template">
			<Setter.Value>
				<ControlTemplate
					TargetType="{x:Type smDesigner:TextContentElementView}">
					<Grid
						Name="PART_LayoutRoot"
						HorizontalAlignment="Stretch">
						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition
									Width="*" />
							</Grid.ColumnDefinitions>
							<core:NineGrid
								Data="{Binding BackgroundImageData}" />
						</Grid>
						<Grid>
							<smDesigner:TransactedTextBox
								Name="PART_TextContent"
								MinHeight="14"
								MinWidth="15"
								VerticalAlignment="Center"
								FontFamily="{DynamicResource DiagramFont}"
								TextBrush="#404040"
								RenderOptions.ClearTypeHint="Enabled"
								FontSize="{StaticResource Stock_DiagramFontSize}"
								Margin="{Binding TextContentMargin}"
								TextAlignment="{Binding TextContentTextAlignment}"
								Text="{Binding TransactedText}"
								HorizontalAlignment="{Binding TextHorizontalAlignment}"
								AcceptsReturn="{Binding AcceptsReturn}"
								AcceptsTab="{Binding AcceptsTab}"
								IsHitTestVisible="{Binding IsReadOnly, Converter={StaticResource BoolToNotBoolConverter}}" />
						</Grid>
						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition
									Width="*" />
							</Grid.ColumnDefinitions>
							<core:NineGrid
								Name="PART_Foreground"
								IsHitTestVisible="False"
								Data="{Binding ForegroundImageData}" />
							<ContentPresenter
								Content="{Binding Content}" />
						</Grid>
						<core:NineGrid
							IsHitTestVisible="False"
							DataSource="{Binding GlyphData}" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
